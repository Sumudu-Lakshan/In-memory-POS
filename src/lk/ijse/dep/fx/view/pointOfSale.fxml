<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="ancPointOfSale" prefHeight="400.0" prefWidth="579.0" style="-fx-background-color: RGB(230,255,255);" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep.fx.controller.ControllerForPointOfSale">
   <children>
      <Label layoutX="104.0" layoutY="29.0" prefHeight="92.0" prefWidth="372.0" text="Point Of Sale">
         <font>
            <Font name="Euphemia" size="36.0" />
         </font>
         <graphic>
            <ImageView fitHeight="75.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../asset/icons8_Cash_in_Hand_48px.png" />
               </image>
            </ImageView>
         </graphic>
      </Label>
      <Button fx:id="btnAddCustomer" layoutX="46.0" layoutY="185.0" mnemonicParsing="false" onAction="#newCustomer_OnAction" prefHeight="44.0" prefWidth="87.0">
         <graphic>
            <ImageView fitHeight="36.0" fitWidth="54.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../asset/icons8_User_96px.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="btnAddItem" layoutX="146.0" layoutY="185.0" mnemonicParsing="false" onAction="#addItem_OnAction" prefHeight="44.0" prefWidth="87.0">
         <graphic>
            <ImageView fitHeight="27.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../asset/icons8_Todo_List_96px.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="btnPlaceOrder" layoutX="254.0" layoutY="184.0" mnemonicParsing="false" onAction="#placeOrder" prefHeight="46.0" prefWidth="87.0">
         <graphic>
            <ImageView fitHeight="38.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../asset/icons8_Add_Shopping_Cart_96px.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="btnViewOrder" layoutX="358.0" layoutY="184.0" mnemonicParsing="false" onAction="#ViewOrder" prefHeight="47.0" prefWidth="80.0">
         <graphic>
            <ImageView fitHeight="39.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../asset/icons8_Eye_96px.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="btnSettings" layoutX="460.0" layoutY="185.0" mnemonicParsing="false" onAction="#goTOsettings" prefHeight="44.0" prefWidth="80.0">
         <graphic>
            <ImageView fitHeight="36.0" fitWidth="71.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../asset/icons8_Settings_96px.png" />
               </image></ImageView>
         </graphic></Button>
      <Button fx:id="btnBack" layoutX="488.0" layoutY="346.0" mnemonicParsing="false" onAction="#Back" text="Back" />
   </children>
</AnchorPane>
